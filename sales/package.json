{
  "name": "sales",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && npm run build:mf && npm run build:npm",
    "build:npm": "babel src --out-dir build --extensions .js,.jsx,.ts,.tsx",
    "build:mf": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:start": "cd dist && PORT=3005 npx serve",
    "start": "webpack serve --open --mode development",
    "start:live": "webpack serve --open --mode development --live-reload --hot"
  },
  "license": "MIT",
  "author": {
    "name": "Jonathas Rodrigues",
    "email": "jonathasprodrigues@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "style-loader": "^3.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@eduzz/houston-forms": "^0.47.5",
    "@eduzz/houston-hooks": "^0.47.5",
    "@eduzz/houston-styles": "^0.47.5",
    "@eduzz/houston-ui": "^0.47.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
