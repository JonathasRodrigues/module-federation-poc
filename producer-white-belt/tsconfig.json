{
    "compilerOptions": {
    "jsx": "react",
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"rootDir": "src",
		"outDir": "build"
	},
	"include": [
		"src"
	]
}